File path: /p5js-course/p5js-course/chapters/0Randomness/demo2/sketch.js

File contents:
let x, y;

function setup() {
    createCanvas(400, 400);
    x = random(width);
    y = random(height);
}

function draw() {
    background(220);
    fill(255, 0, 0);
    ellipse(x, y, 50, 50);
}